{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "chadfernandez-me",
  "main": "dist/_worker.js/index.js",
  "compatibility_date": "2025-12-05",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "workers_dev": false,
  "keep_vars": true,
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "placement": {
    "mode": "smart"
  },
  "observability": {
    "enabled": true
  }
}
